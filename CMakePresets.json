{
  "version": 6,
  "include": [
  ],
  "configurePresets": [
    {
      "name": "fortran-conda-shared",
      "hidden": true,
      "cacheVariables": {
        "CMAKE_BUILD_TYPE": "Release",
        "HDF5_BUILD_DOC": "OFF",
        "HDF5_BUILD_FORTRAN": "ON",
        "HDF5_BUILD_CPP_LIB": "ON",
        "BUILD_SHARED_LIBS": "ON",
        "BUILD_STATIC_LIBS": "OFF",
        "ONLY_SHARED_LIBS": "ON",
        "HDF5_BUILD_HL_LIB": "ON",
        "HDF5_BUILD_TOOLS": "ON",
        "HDF5_ENABLE_THREADSAFE": "ON",
        "ALLOW_UNSUPPORTED": "ON"
      }
    },
    {
      "name": "fortran-conda-gcc-gfortran",
      "generator": "Ninja Multi-Config",
      "inherits": "fortran-conda-shared",
      "hidden": false,
      "environment": {
        "PREFIX": "C:/work/mambaforge/envs/gnu"
      },
      "cacheVariables": {
        "CMAKE_PREFIX_PATH": "$env{PREFIX}/Library:$env{PREFIX}/Library/mingw-w64/lib:$env{PREFIX}/Library/mingw-w64/include:$env{PREFIX}/Library/mingw-w64/bin:$env{PREFIX}/Library/lib:$env{PREFIX}/Library/include",
        "LD_LIBRARY_PATH": "$env{PREFIX}/Library/mingw-w64/lib:$env{PREFIX}/Library/lib",
        "CMAKE_C_COMPILER": "$env{PREFIX}/Library/mingw-w64/bin/gcc.exe",
        "CMAKE_CXX_COMPILER": "$env{PREFIX}/Library/mingw-w64/bin/g++",
        "CMAKE_Fortran_COMPILER": "$env{PREFIX}/Library/mingw-w64/bin/gfortran.exe"
      }
    },
    {
      "name": "fortran-conda-flang",
      "generator": "Ninja Multi-Config",
      "inherits": "fortran-conda-shared",
      "hidden": false,
      "environment": {
        "PREFIX": "C:/work/mambaforge/envs/cflang"
      },
      "cacheVariables": {
        "CMAKE_PREFIX_PATH": "$env{PREFIX}/Library:$env{PREFIX}/Library/bin:$env{PREFIX}/Library/lib:$env{PREFIX}/Library/include",
        "CMAKE_Fortran_COMPILER": "$env{PREFIX}/Library/bin/flang.exe"
      }
    }
  ]
}